
@{
    ViewBag.Title = "お問い合わせ";
    ViewBag.BodyClass = "ContactUs";
    ViewBag.Target = "お問い合わせ";
    Layout = "~/Views/Shared/Layout.cshtml";
}

@section Scripts {
    <script src="~/Scripts/googleReCAPTCHA.js"></script>
    <script src='https://www.google.com/recaptcha/api.js?onload=onloadCallback&render=explicit'></script>
    <script src="https://maps.googleapis.com/maps/api/js?sensor=false"></script>
    <script src="~/Content/js/map.js"></script>
    <script src="~/Scripts/src/tsx/w-contactus_email.js?v=@CommWebSetup.CacheVer"></script>
}

<form action="" class="contacts" id="ContactUsInfo">
    <h3 class="font-sp">お問い合わせへようこそ</h3>

    <label class="col-6">
        <em>*</em> テーマ
        <input type="text" id="m_title" required>
    </label>

    <label class="col-6">
        <em>*</em> 名前
        <input type="text" id="m_name" required>
    </label>

    <label class="col-6">
        <em>*</em> 会社
        <input type="text" id="m_company" required>
    </label>

    <label class="col-6">
        電話番号
        <input type="tel" id="m_tel">
    </label>

    <label><em>*</em> メール</label>
    <input type="email" id="m_email" required>

    <label><em>*</em> この中で記入してください</label>
    <textarea rows="8" id="m_content" required></textarea>

    <div id="Validate"></div>

    <button class="btn btn-small">送る</button>
</form>
<div id="map-canvas"></div>